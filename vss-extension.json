{
    "manifestVersion": 1,
    "id": "azure-devops-extensions-custom-hub",
    "version": "1.0.261",
    "name": "Custom Hub",
    "description": "Custom hub for Azure DevOps",
    "publisher": "jriri",
    "categories": [
        "Azure Pipelines"
    ],
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "icons": {
        "default": "img/logo.png"
    },
    "contributions": [
        {
            "id": "CustomHub",
            "type": "ms.vss-web.hub",
            "targets": [
                "ms.vss-build-web.build-release-hub-group"
            ],
            "properties": {
                "name": "Custom Hub",
                "order": 99,
                "catalogIconUrl": "img/CatalogIcon.png",
                "previewImageUrl": "img/preview.png",
                "uri": "custom-hub.html"
            }
        }
    ],
    "scopes": [
        "vso.build",
        "vso.build_execute",
        "vso.code"
    ],
    "files": [
        {
            "path": "custom-hub.html",
            "addressable": true
        },
        {
            "path": "css",
            "addressable": true
        },
        {
            "path": "sdk/scripts",
            "addressable": true
        },
        {
            "path": "img",
            "addressable": true
        }
    ]
}