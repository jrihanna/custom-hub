{
    "manifestVersion": 1,
    "id": "azure-devops-extensions-pipelines-manager",
    "version": "1.0.5",
    "name": "Pipelines Manager",
    "description": "Pipelines Manager",
    "publisher": "jriri",
    "public": true,
    "content": {
        "details": {
            "path": "overview.md"
        }
    },
    "categories": [
        "Azure Pipelines"
    ],
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "icons": {
        "default": "img/pipelines-logo.png"
    },
    "contributions": [
        {
            "id": "pipelineManager",
            "type": "ms.vss-web.hub",
            "targets": [
                "ms.vss-build-web.build-release-hub-group"
            ],
            "properties": {
                "name": "Pipelines Manager",
                "order": 99,
                "catalogIconUrl": "img/pipelines-logo.png",
                "previewImageUrl": "img/preview.png",
                "uri": "pipeline-manager.html"
            }
        }
    ],
    "scopes": [
        "vso.build",
        "vso.build_execute",
        "vso.code"
    ],
    "files": [
        {
            "path": "pipeline-manager.html",
            "addressable": true
        },
        {
            "path": "css",
            "addressable": true
        },
        {
            "path": "sdk/scripts",
            "addressable": true
        },
        {
            "path": "img",
            "addressable": true
        },
        {
            "path": "overview.md",
            "addressable": true
        }
    ]
}